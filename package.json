{
  "name": "nomadnet-ecommerce",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "clean": "next clean && rm -rf node_modules/.cache",
    "type-check": "tsc --noEmit",
    "kill-port": "sudo lsof -ti:3000 | xargs kill -9",
    "restart": "npm run kill-port && npm run dev"
  },
  "dependencies": {
    "next": "14.2.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "lucide-react": "^0.263.1",
    "stripe": "^12.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "typescript": "^5.2.2",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-config-next": "14.2.32"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  }
}